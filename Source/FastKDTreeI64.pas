function KDT16DI64Distance(const v1, v2: TKDT16DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to TKDT16DI64_AxisCount - 1 do
      Result := Result + KDT16DI64Pow(v2[i] - v1[i]);
