var
  TKDT96DI64_Test    : TKDT96DI64;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT96DI64_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
