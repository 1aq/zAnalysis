class procedure TKDT640DI64.Test;
var
  TKDT640DI64_Test    : TKDT640DI64;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT640DI64_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;

begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT640DI64_Test := TKDT640DI64.Create;

  DoStatusNoLn('...');
  SetLength(TKDT640DI64_Test.TestBuff, 300);
  for i := 0 to length(TKDT640DI64_Test.TestBuff) - 1 do
    for j := 0 to TKDT640DI64_AxisCount - 1 do
        TKDT640DI64_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT640DI64_Test.TestBuff), length(TKDT640DI64_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT640DI64_Test.BuildKDTreeM(False, length(TKDT640DI64_Test.TestBuff), @TKDT640DI64_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT640DI64_Test.BuildKDTreeM(False, length(TKDT640DI64_Test.TestBuff), TKDT640DI64_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT640DI64_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT640DI64_Test.TestBuff));
  TKDT640DI64_Test.Search(TKDT640DI64_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT640DI64Distance(TKDT640DI64_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT640DI64_Test.Clear;
  { kMean test }
  TKDT640DI64_Test.BuildKDTreeWithCluster(TKDT640DI64_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT640DI64_Test.Search(TKDT640DI64_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT640DI64_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT640DI64_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT640DI64_Test);
end;

