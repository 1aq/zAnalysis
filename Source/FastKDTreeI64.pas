procedure TKDT640DI64.BuildKDTreeWithClusterM(const PlanCount, k, Restarts: NativeInt; var OutIndex: TDynamicIndexArray; const OnTrigger: TKDT640DI64_BuildMethod);
var
  TempStoreBuff: TKDT640DI64yanmicStoreBuffer;
