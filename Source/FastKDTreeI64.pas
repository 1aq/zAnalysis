function KDT22DI64Distance(const v1, v2: TKDT22DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to TKDT22DI64_AxisCount - 1 do
      Result := Result + KDT22DI64Pow(v2[i] - v1[i]);
