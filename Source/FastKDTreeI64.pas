function KDT36DI64Distance(const v1, v2: TKDT36DI64_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to TKDT36DI64_AxisCount - 1 do
      Result := Result + KDT36DI64Pow(v2[i] - v1[i]);
