class procedure TKDT40DI8.Test;
var
  TKDT40DI8_Test    : TKDT40DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT40DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;

begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT40DI8_Test := TKDT40DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT40DI8_Test.TestBuff, 300);
  for i := 0 to length(TKDT40DI8_Test.TestBuff) - 1 do
    for j := 0 to TKDT40DI8_AxisCount - 1 do
        TKDT40DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT40DI8_Test.TestBuff), length(TKDT40DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT40DI8_Test.BuildKDTreeM(False, length(TKDT40DI8_Test.TestBuff), @TKDT40DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT40DI8_Test.BuildKDTreeM(False, length(TKDT40DI8_Test.TestBuff), TKDT40DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT40DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT40DI8_Test.TestBuff));
  TKDT40DI8_Test.Search(TKDT40DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT40DI8Distance(TKDT40DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT40DI8_Test.Clear;
  { kMean test }
  TKDT40DI8_Test.BuildKDTreeWithCluster(TKDT40DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT40DI8_Test.Search(TKDT40DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT40DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT40DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT40DI8_Test);
end;

