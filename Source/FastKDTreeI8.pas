function KDT16DI8Vec(const s: string): TKDT16DI8_Vec;
var
  t          : TTextParsing;
  SplitOutput: TArrayPascalString;
  i, j       : NativeInt;
begin
  for i := 0 to TKDT16DI8_AxisCount - 1 do
      Result[i] := 0;

  t := TTextParsing.Create(s, tsText, nil);
  if t.SplitChar(1, ', ', '', SplitOutput) > 0 then
    begin
      j := 0;
      for i := 0 to length(SplitOutput) - 1 do
        if umlGetNumTextType(SplitOutput[i]) <> ntUnknow then
          begin
            Result[j] := umlStrToInt(SplitOutput[i], 0);
            inc(j);
            if j >= TKDT16DI8_AxisCount then
                break;
          end;
    end;
  DisposeObject(t);
end;

function KDT16DI8Vec(const v: TKDT16DI8_Vec): string;
var
  i: NativeInt;
begin
  Result := '';
  for i := 0 to TKDT16DI8_AxisCount - 1 do
    begin
      if i > 0 then
          Result := Result + ',';
      Result := Result + umlIntToStr(v[i]);
    end;
end;

function KDT16DI8Pow(const v: TKDT16DI8_VecType): Double;
begin
