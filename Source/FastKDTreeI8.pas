class procedure TKDT1536DI8.Test;
var
  TKDT1536DI8_Test    : TKDT1536DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT1536DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;

begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT1536DI8_Test := TKDT1536DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT1536DI8_Test.TestBuff, 300);
  for i := 0 to length(TKDT1536DI8_Test.TestBuff) - 1 do
    for j := 0 to TKDT1536DI8_AxisCount - 1 do
        TKDT1536DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT1536DI8_Test.TestBuff), length(TKDT1536DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT1536DI8_Test.BuildKDTreeM(False, length(TKDT1536DI8_Test.TestBuff), @TKDT1536DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT1536DI8_Test.BuildKDTreeM(False, length(TKDT1536DI8_Test.TestBuff), TKDT1536DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT1536DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT1536DI8_Test.TestBuff));
  TKDT1536DI8_Test.Search(TKDT1536DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT1536DI8Distance(TKDT1536DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT1536DI8_Test.Clear;
  { kMean test }
  TKDT1536DI8_Test.BuildKDTreeWithCluster(TKDT1536DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT1536DI8_Test.Search(TKDT1536DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT1536DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT1536DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT1536DI8_Test);
end;

