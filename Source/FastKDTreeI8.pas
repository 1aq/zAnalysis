  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT96DI8_Test := TKDT96DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT96DI8_Test.TestBuff, 300);
  for i := 0 to length(TKDT96DI8_Test.TestBuff) - 1 do
    for j := 0 to TKDT96DI8_AxisCount - 1 do
        TKDT96DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT96DI8_Test.TestBuff), length(TKDT96DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT96DI8_Test.BuildKDTreeM(False, length(TKDT96DI8_Test.TestBuff), @TKDT96DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT96DI8_Test.BuildKDTreeM(False, length(TKDT96DI8_Test.TestBuff), TKDT96DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT96DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT96DI8_Test.TestBuff));
  TKDT96DI8_Test.Search(TKDT96DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT96DI8Distance(TKDT96DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT96DI8_Test.Clear;
  { kMean test }
  TKDT96DI8_Test.BuildKDTreeWithCluster(TKDT96DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT96DI8_Test.Search(TKDT96DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT96DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT96DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT96DI8_Test);
end;

