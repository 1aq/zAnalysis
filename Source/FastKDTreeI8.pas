function KDT30DI8Distance(const v1, v2: TKDT30DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to TKDT30DI8_AxisCount - 1 do
      Result := Result + KDT30DI8Pow(v2[i] - v1[i]);
