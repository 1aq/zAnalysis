function KDT36DI8Distance(const v1, v2: TKDT36DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to TKDT36DI8_AxisCount - 1 do
      Result := Result + KDT36DI8Pow(v2[i] - v1[i]);
