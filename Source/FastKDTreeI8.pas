class procedure TKDT32DI8.Test;
var
  TKDT32DI8_Test    : TKDT32DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT32DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;

begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT32DI8_Test := TKDT32DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT32DI8_Test.TestBuff, 300);
  for i := 0 to length(TKDT32DI8_Test.TestBuff) - 1 do
    for j := 0 to TKDT32DI8_AxisCount - 1 do
        TKDT32DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT32DI8_Test.TestBuff), length(TKDT32DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT32DI8_Test.BuildKDTreeM(False, length(TKDT32DI8_Test.TestBuff), @TKDT32DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT32DI8_Test.BuildKDTreeM(False, length(TKDT32DI8_Test.TestBuff), TKDT32DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT32DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT32DI8_Test.TestBuff));
  TKDT32DI8_Test.Search(TKDT32DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT32DI8Distance(TKDT32DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT32DI8_Test.Clear;
  { kMean test }
  TKDT32DI8_Test.BuildKDTreeWithCluster(TKDT32DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT32DI8_Test.Search(TKDT32DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT32DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT32DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT32DI8_Test);
end;

