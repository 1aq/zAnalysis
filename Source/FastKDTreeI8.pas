  for i := 0 to length(inBuff) - 1 do
    for j := 0 to TKDT768DI8_AxisCount - 1 do
        Source[i, j] := inBuff[i, j];

  if KMeansCluster(Source, TKDT768DI8_AxisCount, k, umlMax(Restarts, 1), KArray, OutIndex) = 1 then
