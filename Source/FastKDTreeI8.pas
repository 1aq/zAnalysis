function KDT1536DI8Distance(const v1, v2: TKDT1536DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to TKDT1536DI8_AxisCount - 1 do
      Result := Result + KDT1536DI8Pow(v2[i] - v1[i]);
