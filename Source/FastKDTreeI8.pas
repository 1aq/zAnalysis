class procedure TKDT768DI8.Test;
var
  TKDT768DI8_Test    : TKDT768DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT768DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;

begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT768DI8_Test := TKDT768DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT768DI8_Test.TestBuff, 300);
  for i := 0 to length(TKDT768DI8_Test.TestBuff) - 1 do
    for j := 0 to TKDT768DI8_AxisCount - 1 do
        TKDT768DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT768DI8_Test.TestBuff), length(TKDT768DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT768DI8_Test.BuildKDTreeM(False, length(TKDT768DI8_Test.TestBuff), @TKDT768DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT768DI8_Test.BuildKDTreeM(False, length(TKDT768DI8_Test.TestBuff), TKDT768DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT768DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT768DI8_Test.TestBuff));
  TKDT768DI8_Test.Search(TKDT768DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT768DI8Distance(TKDT768DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT768DI8_Test.Clear;
  { kMean test }
  TKDT768DI8_Test.BuildKDTreeWithCluster(TKDT768DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT768DI8_Test.Search(TKDT768DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT768DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT768DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT768DI8_Test);
end;

