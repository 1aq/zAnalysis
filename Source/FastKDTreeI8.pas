        TKDT2048DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT2048DI8_Test.TestBuff), length(TKDT2048DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT2048DI8_Test.BuildKDTreeM(False, length(TKDT2048DI8_Test.TestBuff), @TKDT2048DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT2048DI8_Test.BuildKDTreeM(False, length(TKDT2048DI8_Test.TestBuff), TKDT2048DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT2048DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT2048DI8_Test.TestBuff));
  TKDT2048DI8_Test.Search(TKDT2048DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT2048DI8Distance(TKDT2048DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT2048DI8_Test.Clear;
  { kMean test }
  TKDT2048DI8_Test.BuildKDTreeWithCluster(TKDT2048DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT2048DI8_Test.Search(TKDT2048DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT2048DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT2048DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT2048DI8_Test);
end;

