class procedure TKDT1920DI8.Test;
var
  TKDT1920DI8_Test    : TKDT1920DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT1920DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;

begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT1920DI8_Test := TKDT1920DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT1920DI8_Test.TestBuff, 300);
  for i := 0 to length(TKDT1920DI8_Test.TestBuff) - 1 do
    for j := 0 to TKDT1920DI8_AxisCount - 1 do
        TKDT1920DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT1920DI8_Test.TestBuff), length(TKDT1920DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT1920DI8_Test.BuildKDTreeM(False, length(TKDT1920DI8_Test.TestBuff), @TKDT1920DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT1920DI8_Test.BuildKDTreeM(False, length(TKDT1920DI8_Test.TestBuff), TKDT1920DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT1920DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT1920DI8_Test.TestBuff));
  TKDT1920DI8_Test.Search(TKDT1920DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT1920DI8Distance(TKDT1920DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT1920DI8_Test.Clear;
  { kMean test }
