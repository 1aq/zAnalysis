  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT2048DI8_Test := TKDT2048DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT2048DI8_Test.TestBuff, 300);
