class procedure TKDT372DI8.Test;
var
  TKDT372DI8_Test    : TKDT372DI8;
  t              : TTimeTick;
  i, j           : NativeInt;
  TestResultBuff : TKDT372DI8_DynamicVecBuffer;
  TestResultIndex: TDynamicIndexArray;
  KMeanOutIndex  : TDynamicIndexArray;
  errored        : Boolean;

begin
  errored := False;
  DoStatusNoLn('test %s...', [ClassName]);
  t := GetTimeTick;

  TKDT372DI8_Test := TKDT372DI8.Create;

  DoStatusNoLn('...');
  SetLength(TKDT372DI8_Test.TestBuff, 300);
  for i := 0 to length(TKDT372DI8_Test.TestBuff) - 1 do
    for j := 0 to TKDT372DI8_AxisCount - 1 do
        TKDT372DI8_Test.TestBuff[i][j] := umlRandomRange(-length(TKDT372DI8_Test.TestBuff), length(TKDT372DI8_Test.TestBuff));

  DoStatusNoLn('...');
  {$IFDEF FPC}
  TKDT372DI8_Test.BuildKDTreeM(False, length(TKDT372DI8_Test.TestBuff), @TKDT372DI8_Test.Test_BuildM);
  {$ELSE FPC}
  TKDT372DI8_Test.BuildKDTreeM(False, length(TKDT372DI8_Test.TestBuff), TKDT372DI8_Test.Test_BuildM);
  {$ENDIF FPC}
  { parallel search test }
  DoStatusNoLn('...');
  SetLength(TestResultBuff, length(TKDT372DI8_Test.TestBuff));
  SetLength(TestResultIndex, length(TKDT372DI8_Test.TestBuff));
  TKDT372DI8_Test.Search(TKDT372DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if KDT372DI8Distance(TKDT372DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
        errored := True;

  DoStatusNoLn('...');
  TKDT372DI8_Test.Clear;
  { kMean test }
  TKDT372DI8_Test.BuildKDTreeWithCluster(TKDT372DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
  { parallel search test }
  TKDT372DI8_Test.Search(TKDT372DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

  for i := 0 to length(TestResultIndex) - 1 do
    if TestResultIndex[i] <> KMeanOutIndex[i] then
        errored := True;

  SetLength(TKDT372DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT372DI8_Test.Clear;

  if errored then
      DoStatusNoLn('error!')
  else
      DoStatusNoLn('passed ok %dms', [GetTimeTick - t]);
  DoStatusNoLn;

  DisposeObject(TKDT372DI8_Test);
end;

