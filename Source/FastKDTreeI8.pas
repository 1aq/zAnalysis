procedure TKDT640DI8.BuildKDTreeWithClusterM(const PlanCount, k, Restarts: NativeInt; var OutIndex: TDynamicIndexArray; const OnTrigger: TKDT640DI8_BuildMethod);
var
  TempStoreBuff: TKDT640DI8yanmicStoreBuffer;
